<h1 align="center"> Edit post </h1>

<%= form_for(@post) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <p>
    <%= f.text_area :body, placeholder: "edit post..." %>
  </p>
  <%= f.submit "Post"%>
  <%= f.file_field :picture, accept: 'image/jpeg,image/gif,image/png' %>
<% end %>