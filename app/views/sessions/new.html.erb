<%unless logged_in?%>
<h1>Log in</h1>
<%= form_for(:session, url: login_path) do |f| %>
	</p>
		<%= f.label :email %>
		<%= f.email_field :email%>
	</p>
	<p>
		<%= f.label :password %>
		<%= f.password_field :password%>
	</p>
	<p>
		<%= f.submit "Log in"%>
	</p>
<% end %>

<p>New user? <%= link_to "Sign up now!", signup_path %></p>
<%else%>
	<h4>you've already logged in</h4>
	<%=link_to "go to profile", user_path(current_user)%>
<%end%>